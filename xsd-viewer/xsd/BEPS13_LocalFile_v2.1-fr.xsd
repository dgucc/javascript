			<xs:element name="Name" type="CompanyName_Type">
				<xs:annotation>

					<xs:documentation xml:lang="fr">Dénomination</xs:documentation>


				</xs:annotation>
			</xs:element>
			<xs:element name="CountryCode" type="iso:CountryCode_Type">
				<xs:annotation>

					<xs:documentation xml:lang="fr">Code pays</xs:documentation>


				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="A7CoreData_Type">
		<xs:annotation>

			<xs:documentation xml:lang="fr">Données principales (A7)</xs:documentation>


		</xs:annotation>
		<xs:sequence>
			<xs:element name="UltimateParentCompany" type="UltimateParentCompany_Type">
				<xs:annotation>

					<xs:documentation xml:lang="fr">Entité mère ultime</xs:documentation>


				</xs:annotation>
			</xs:element>
			<xs:element name="ReportingTree" type="ReportingTree_Type">
