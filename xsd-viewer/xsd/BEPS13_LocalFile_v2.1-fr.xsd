		<xs:annotation>

			<xs:documentation xml:lang="fr">Période imposables</xs:documentation>


		</xs:annotation>
		<xs:sequence>
			<xs:element name="TaxPeriod_0" type="SignedEuroAmount_Type" minOccurs="0">
				<xs:annotation>

					<xs:documentation xml:lang="fr">Période imposable-0</xs:documentation>


				</xs:annotation>
			</xs:element>
			<xs:element name="TaxPeriod_1" type="SignedEuroAmount_Type" minOccurs="0">
				<xs:annotation>

					<xs:documentation xml:lang="fr">Période imposable-1</xs:documentation>


				</xs:annotation>
			</xs:element>
			<xs:element name="TaxPeriod_2" type="SignedEuroAmount_Type" minOccurs="0">
				<xs:annotation>

					<xs:documentation xml:lang="fr">Période imposable-2</xs:documentation>


				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
