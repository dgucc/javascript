					<xs:documentation xml:lang="fr">Restructurations</xs:documentation>


				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="UltimateParentCompany_Type">
		<xs:annotation>

			<xs:documentation xml:lang="fr">Entité mère ultime</xs:documentation>


		</xs:annotation>
		<xs:sequence>
			<xs:element name="Name" type="CompanyName_Type">
				<xs:annotation>

					<xs:documentation xml:lang="fr">Nom de l'entité mère ultime</xs:documentation>


				</xs:annotation>
			</xs:element>
			<xs:element name="CountryCode" type="iso:CountryCode_Type">
				<xs:annotation>

					<xs:documentation xml:lang="fr">Code de pays de l'entité mère ultime</xs:documentation>


				</xs:annotation>
			</xs:element>
			<xs:element name="TIN" type="TIN_Type">
				<xs:annotation>

					<xs:documentation xml:lang="fr">Numéro d'identification fiscale de l'entité mère ultime</xs:documentation>


				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="ReportingTree_Type">
		<xs:annotation>

			<xs:documentation xml:lang="fr">Structure du reporting</xs:documentation>


		</xs:annotation>
		<xs:sequence>
			<xs:element name="AmountOfBusinessUnits" type="AmountOfBusinessUnits_Type">
