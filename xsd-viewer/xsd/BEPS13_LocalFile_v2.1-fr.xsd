					<xs:documentation xml:lang="fr">Entreprise employant cette personne</xs:documentation>


				</xs:annotation>
			</xs:element>
			<xs:element name="CountryOfReceiversEmployer" type="iso:CountryCode_Type">
				<xs:annotation>

					<xs:documentation xml:lang="fr">Pays dans lequel cette entreprise est établie</xs:documentation>


				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="A5CompanyActivities_Type">
