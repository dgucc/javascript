			<xs:element name="Attachment" type="IsAttachmentAdded_Type" minOccurs="0">
				<xs:annotation>

					<xs:documentation xml:lang="fr">PDF ajouté à la déclaration</xs:documentation>


				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="A4ReportingTree_Type">
		<xs:annotation>

			<xs:documentation xml:lang="fr">Structure du rapportage belge (A4)</xs:documentation>


		</xs:annotation>
		<xs:sequence>
			<xs:element name="Function" type="Function_Type">
