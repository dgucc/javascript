			<xs:element name="B1NatureOfActivities" type="B1NatureOfActivities_Type"  maxOccurs="unbounded">
				<xs:annotation>

					<xs:documentation xml:lang="fr">Nature des activités par unité d'exploitation (B1)</xs:documentation>


				</xs:annotation>
			</xs:element>
			<xs:element name="B2AllTransactions" type="B2AllTransactions_Type" maxOccurs="unbounded">
				<xs:annotation>

					<xs:documentation xml:lang="fr">Données des transactions, tant liées que non liées, par unité d'exploitation (B2)</xs:documentation>


				</xs:annotation>
			</xs:element>
			<xs:element name="B3ControlledGoodsTransactions" type="B3ControlledGoodsTransactions_Type" minOccurs="0" maxOccurs="unbounded">
