					<xs:documentation xml:lang="fr">Stratégie d'entreprise</xs:documentation>


				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="A6KeyCompetitors_Type">
